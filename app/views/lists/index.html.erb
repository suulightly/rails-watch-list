<%= render 'navbar' %>

<div class="home-banner">
  <h1>Save Movie Lists</h1>
</div>

<div class="container mt-3">
  <h3>here are all the lists</h3>

  <div class="cards">
    <% @lists.each do |list| %>
      <div class="card-category" style="background-color: black">
        <%= link_to list.name, list_path(list) %>
      </div>
    <% end %>
  </div>
</div>
